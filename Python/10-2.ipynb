   "source": [
    "import sympy as sympy\n",
    "x = sympy.Symbol('X')\n",
    "mu = sympy.integrate(x*1/2, (x, -1, 1))\n",
    "mu\n",
    "EX2 = sympy.integrate(x**2*1/2, (x, -1, 1))\n",
    "EX2\n",
    "sigma2 = EX2 - mu**2\n",
    "sigma2\n",
    "sigma = sympy.sqrt(sigma2)\n",
    "sigma"
   ]
  }
 ]
